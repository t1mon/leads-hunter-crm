{
    "name": "t1mon/leads-hunter-crm",
    "description": "Laravel 8.0 CRM LEADS.",
    "keywords": ["framework", "laravel", "crm"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-debugbar": "3.5.1",
        "doctrine/dbal": "^3.2",
        "facade/ignition": "^2.16",
        "fideloper/proxy": "4.4.0",
        "fruitcake/laravel-cors": "2.0.2",
        "joselfonseca/laravel-tactician": "^1.0",
        "laravel/framework": "8.*",
        "laravel/horizon": "^5.9.3",
        "laravel/socialite": "v5.2.6",
        "laravel/tinker": "v2.6.3",
        "laravel/ui": "v3.4.0",
        "laravelcollective/html": "6.2.0",
        "maatwebsite/excel": "^3.1",
        "nunomaduro/collision": "v5.6.0",
        "predis/predis": "1.1.6",
        "pusher/pusher-php-server": "7.0.2",
        "spatie/laravel-backup": "7.7.3",
        "spatie/laravel-medialibrary": "9.11.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "filp/whoops": "2.14.3",
        "friendsofphp/php-cs-fixer": "v2.19.3",
        "laravel/homestead": "12.8.0",
        "laravel/telescope": "v4.6.9",
        "mockery/mockery": "1.4.2",
        "phpunit/phpunit": "9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/date.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
